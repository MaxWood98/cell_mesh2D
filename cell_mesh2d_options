#cell_mesh2d options file (version 0.9.5) 
  
#=== General Options ================================= 
#Toggle console display (1 = yes || 0 = no) 
condisp = 1 
  
#=== Mesh Format Options ============================= 
#Type of mesh (cutcell / su2_cutcell / su2_dual / minD_O) 
meshtype = cutcell 
  
#Mesh inside or outside of geometry 
meshinout = out 
  
#Surface normal direction switch in to / out of the mesh domain 
surfnormdir = in 
  
#Boundary normal direction switch in to / out of the mesh domain 
bndrynormdir = in 
  
#=== Quadtree Options ================================ 
#Maximum refinement level 
nqtrefine = 12 
  
#Maximum additional refinement levels in high curvature regions 
nboostqtrefine = 0 
  
#Maximum number of cells 
ncellmax = 200000 
  
#Refinement adjacency flooding iterations at the first refinement 
nadjfloodi = 7 
  
#Refinement adjacency flooding iterations at the final refinement 
nadjfloodf = 7 
  
#Refinement adjacency flooding iterations on boosted refinement 
nadjfloodb = 2 
  
#Far field distance from object centre 
farfielddist = 20.000000 
  
#Object/mesh centre offset (x / y) 
offsett_x = 0.000000 
offsett_y = 0.000000 
  
#=== Custom Domain Bound Specification  ============== 
#Enable custom mesh domain bounds (1 = yes | 0 = no) 
forcebounds =  0 
  
#Mesh domain bounds (xmin xmax ymin ymax) 
bound_xmin = 2.000000E-01 
bound_xmax = 1.180000E+01 
bound_ymin = 2.000000E-01 
bound_ymax = 1.180000E+01 
  
#=== Mesh Cleaning Options =========================== 
#Minimum edge length as a fraction of an undeformed cell edge length at each refienemnt level 
eminlength = 1.000000E-08 
  
#Minimum surface edge length as a fraction of an undeformed cell edge length at each refienemnt level 
srfeminlength = 5.000000E-03 
  
#Volume fraction of an undeformed cell at each refinement level below which a cell is classed as a sliver cell 
cminvol = 1.000000e-01 
  
#Allow cleaning of the input surface geometry  
srfinclean = no 
  
#=== Mesh Geometry Intersection Options ============== 
#Maximum number of mesh-geometry intersections on each mesh edge 
enintmax = 50 
  
#Edge-geometry intersection search zone padding as a fraction of the edge length 
eintpad = 0.000000E+00 
  
#Intersection co-incidence tollerance as fraction of item length 
intcointol = 1.000000E-08 
  
#=== Surface Format Options ========================== 
#Geometry surface type (0 = simplified | 1 = exact) 
surftype = 0 
  
#Surface curvature multiplier 
scurvmult = 1.000000 
  
#Number of vertices used to estimate local surface curvaturer 
scurvnpnt = 10 
  
#=== Inflation Layer Options ========================= 
#Toggle construction of an inflation layer (yes | no) 
build_inflayer = no 
  
#Inflation layer approximate height 
inflayer_height = 0.040000 
  
#Number of layers within the inflation layer (determine automatically if zero) 
inflayer_nlayer = 0 
  
#Inflation layer differencing weight (0->1) 
inflayer_wd = 0.100000 
  
#Inflation layer convex differencing penalty (0->1) 
inflayer_cvxdp = 0.100000 
  
#Inflation layer evening weight (0->1) 
inflayer_we = 0.500000 
  
#Inflation layer evening bias base value 
inflayer_ebcbase = 0.100000 
  
#Inflation layer evening number of flood iterations 
inflayer_enflood = 10 
  
#Inflation layer evening number of sub-iterations 
inflayer_ensubiter = 10 
  
#Inflation layer evening length ratio bound (>=1) 
inflayer_lreb = 1.000000 
  
#=== Mesh Smoothing Options ========================== 
#Near surface smoothing type (0 = none | 1 = Laplacian) 
smoothingtype =  0 
  
#Vertex selection flooding iterations from surfaces 
smoothingnflood =  5 
  
#Smoothing iterations 
smoothingniter = 10 
  
#=== ADtree Options ================================== 
#Padding size of adtree search bounding boxes as a multiple of cell edge length 
adtpadding = 0.000000 
  
#AD tree maximum depth as the number of dimension cycles (tree is 4d) 
adtndimcycle = 10 
  
#AD tree minimum divisible node size 
adtmindivnsize = 10 
  
#=== Gradient Interpolation Options ================== 
#Construct volume to surface gradient interpolation (1 = yes | 0 = no) 
glinkconstruct = 0 
  
#Gradient linking type (rbf or int) 
glinktype = int 
  
#Number of nearest neighbours to use for RBF volume to surface gradient interpolation 
glinkrbfnpnt = 80 
  
#Number of vertices each side used to smooth the gradient at each surface vertex 
glinknpntsmooth = 0 
  
#=== Radial Basis Function Options =================== 
#RBF interpolation support radius as a multiple of the maximum distance between points 
rbfrsup = 50.000000 
  
#RBF interpolation relaxation distance as fraction of the maximum distance between points 
rbfrrelax = 0.010000 
  
#RBF interpolation relaxation parameter 
rbfrelaxp = 0.010000 
  
#=== Boundary Condition Options ====================== 
#Set custom boundary conditions in specifed regions (1 = yes | 0 = no) 
setcustombcs =  0 
  
#Remove any region of the mesh connected to a far field boundary condition 
remffczones = 0 
  
#Remove any region of the mesh connected to a non custom set boundary condition 
remncczones = 0 
  
#Remove any isolated region of the mesh connected only to a wall boundary condition 
remwallonlyzones = 0 
  
